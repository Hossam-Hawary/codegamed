angular.module('codeGamed',['ngRoute']).config(function($routeProvider){

    $routeProvider.when('/',{
        templateUrl: "<%= asset_path('levels.html.erb')%>",
        controller: 'levelCtrl'

    });


  $routeProvider.when('/missions',{
    templateUrl: "<%= asset_path('missions.html.erb')%>",
    controller: 'missionCtrl'

  });

});


