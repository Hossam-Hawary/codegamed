 <nav class="navbar primary-color">
   <div class="container-fluid">
     <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-2">
       <ul class="nav navbar-nav">
         <li ng-repeat="mission in missions" ng-click="play_mission(mission,$event)" class="box  waves-effect waves-light"  ng-class="{'animated infinite zoomIn':$last} ">
           <span class=" social-counter">{{mission.mission_data.score}}XP</span> <i class="material-icons">security</i>
         </li>
         <li ng-repeat="n in [] | range:locked_missions" >
           <i class="material-icons icon_small">security</i>
         </li>
       </ul>
         <div class="navbar-right">
           <img ng-src={{badge.image_url.url}} class="codeGamed-toolbar-avatar">
           <i class="material-icons icon_small"  ng-click="openThemeMenu()">settings</i>
         </div>
     </div>
   </div>
 </nav>
<div class="container-fluid">
<div class="row"  >
  <div class="col-lg-6" id="video_part">
    <md-tabs md-border-bottom style="min-height: 100%;">
      <md-tab ng-if="true" label="Video">
        <md-content class="md-padding">

          <iframe class="col-lg-12" ng-src="{{ mission_video | trusted }}"  frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>

        </md-content>
      </md-tab>
      <md-tab label="Problem Description">
        <md-content class="md-padding">
          <p ng-bind-html="myHTML"></p>
        </md-content>
      </md-tab>
    </md-tabs>
</div>
  <div class="col-lg-6">
    <div class="row">
    <md-button class="md-raised primary-color"id="submit" ng-click="submitAnswer()">Submit</md-button>
      </div>
    <div  style=";position:relative;z-index: 0">
        <div id="codeEditing"></div>
      </div>

      <div id="test_cases-div">
        <div ng-repeat="test_case in test_cases" my-cool-directive>
          <h3><strong>Input: </strong>{{test_case.input}}</h3>
          <div>
            <p><strong>Output: </strong>{{test_case.output}}</p>
          </div>
        </div>
      </div>
  </div>
</div>
  </div>

 <md-sidenav  md-component-id="theme" md-is-open="isThemeSidenavOpen" class="md-sidenav-right ">
   <i class="material-icons" ng-click="closeThemeMenu()">cancel</i>
   <md-list layout-padding>
     <md-list-item class="md-3-line" >
       <div class="md-list-item-text">
         <div>
           <label>Theme: </label>
           <md-select ng-model="current_theme" ng-change="update_theme()">

             <md-option ng-repeat="theme in themes" value="{{theme}}">
               {{theme}}
             </md-option>
           </md-select>
         </div>
       </div>
     </md-list-item>

   </md-list>
 </md-sidenav>

<script src="/codemirror.js"></script>