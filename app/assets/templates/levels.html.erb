<div class="levels-grid-background-color"  >
<md-grid-list   md-cols="1"  md-gutter="1em" md-row-height="400px" >

  <md-grid-tile  >


      <img class="background-image" src="1.jpg"  />


  </md-grid-tile>




</md-grid-list>

<br> <br>


<md-grid-list md-cols="3"  md-row-height="400px"  >

  <md-grid-tile  ng-repeat="badge in badges" >

    <md-card>

      <md-card-title>
        <md-card-title-text>
          <span class="md-headline">{{badge.title}}</span>

        </md-card-title-text>

      </md-card-title>


      <md-card-content layout="row" layout-align="space-between">
        <a ng-href="/#/missions/{{badge.level_id}}"> <div class="md-media-xl card-media card-image-resize" style="background-image: url('{{badge.image_url}}'); ">

        </div> </a>

      </md-card-content>


    </md-card>


  </md-grid-tile>






  <md-grid-tile  ng-repeat="locked in locked_levels" >

    <md-card>

      <md-card-title>
        <md-card-title-text>
          <span class="md-headline" style="color: #3E7087">Locked </span>

        </md-card-title-text>

      </md-card-title>


      <md-card-content layout="row" layout-align="space-between">
        <div class="md-media-xl card-media card-image-resize" style="background-image: url('locked.jpg'); ">

        </div>

      </md-card-content>


    </md-card>


  </md-grid-tile>






</md-grid-list>
</div>

